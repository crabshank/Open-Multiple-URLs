{
  "name": "Open Multiple URLs",
  "version": "2023.11",
  "manifest_version": 3,
  "description": "Opens a list of URLs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
	"history"
  ]
}
