{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Open Multiple URLs",
  "version": "2022.12",
  "manifest_version": 2,
  "description": "Opens a list of URLs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
